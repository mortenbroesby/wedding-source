{
  "name": "wedding-source",
  "version": "1.0.0",
  "description": "Source for wedding website",
  "author": "Morten Broesby-Olsen <mortenbroesby@gmail.com>",
  "main": "index.ts",
  "scripts": {
    "eslint": "eslint --fix --ext .js,.vue src",
    "predev": "npm run cleanDev",
    "dev": "parcel serve src/index.html -d dev-public --public-url /dev-public/ --open",
    "clean": "rimraf .cache/ && rimraf public/ && rimraf build/",
    "cleanDev": "rimraf .cache/ && rimraf dev-public/ && rimraf build/",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --public-url / --out-dir ./public --no-source-maps --log-level 3",
    "deploy": "npm run build && firebase deploy --only hosting",
    "unit": "jest --config test/unit/jest.config.js --coverage",
    "unitWatch": "jest --config test/unit/jest.config.js --watch"
  },
  "keywords": [
    "vue",
    "typescript",
    "parcel",
    "scss"
  ],
  "license": "ISC",
  "dependencies": {
    "@types/lodash-es": "^4.17.3",
    "@types/universal-analytics": "0.4.3",
    "axios": "0.19.0",
    "babel-polyfill": "6.26.0",
    "firebase": "7.2.0",
    "get-user-locale": "1.2.0",
    "js-logger": "1.6.0",
    "lodash-es": "^4.17.15",
    "moment": "2.24.0",
    "parcel-plugin-static-files-copy": "^2.2.1",
    "postcss-modules": "1.4.1",
    "universal-analytics": "0.4.20",
    "vue": "2.6.10",
    "vue-class-component": "7.1.0",
    "vue-hot-reload-api": "2.3.4",
    "vue-i18n": "8.14.1",
    "vue-meta": "2.3.1",
    "vue-property-decorator": "8.2.2",
    "vue-router": "3.1.3",
    "vue-toasted": "1.1.27",
    "vuex": "3.1.1",
    "workbox-sw": "^4.3.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.12",
    "@vue/component-compiler-utils": "3.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "autoprefixer": "9.6.4",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.3",
    "babel-helper-vue-jsx-merge-props": "2.0.3",
    "babel-plugin-syntax-jsx": "6.18.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-transform-vue-jsx": "3.7.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-vue": "2.0.2",
    "cross-env": "6.0.3",
    "dart-sass": "1.23.0",
    "eslint": "6.5.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-vue": "2.0.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-vue": "5.2.3",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0",
    "jest": "24.9.0",
    "jest-junit": "8.0.0",
    "jest-serializer-vue": "2.0.2",
    "jest-transform-stub": "2.0.0",
    "marked": "0.7.0",
    "node-sass": "4.12.0",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-eslint": "1.0.7",
    "parcel-plugin-inlinesvg": "0.1.1",
    "sass": "^1.26.10",
    "ts-jest": "24.1.0",
    "tslint": "5.20.0",
    "tslint-config-standard": "8.0.1",
    "typescript": "3.6.4",
    "vue-jest": "3.0.5",
    "vue-template-compiler": "2.6.10"
  },
  "staticFiles": {
    "staticPath": [
      {
        "outDirPattern": "**/dev-public",
        "staticPath": "static",
        "watcherGlob": "**/*"
      },
      {
        "outDirPattern": "**/public",
        "staticPath": "static",
        "watcherGlob": "**/*"
      }
    ]
  },
  "browserslist": [
    "cover 99.5%",
    "last 3 years",
    "not dead"
  ]
}
